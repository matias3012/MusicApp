<h1>Listado de Reproduccion</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Number of Votes</th>
    </tr>
  </thead>

  <tbody>
    <%= @playlists.each do |playlist| %>
      <tr>
        <td><%= playlist.name %></td>
        <td><%= playlist.number_of_votes %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%#= link_to 'New Track', new_playlist_path %>
